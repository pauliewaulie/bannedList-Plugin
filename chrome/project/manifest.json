{
    "name": "#BannedList Highlighter",
    "version": "1.0",
    "description": "Highlight #BannedList phrases on every page you read.",
    "homepage_url": "http://123.writeboard.com/pya47esrbcl89qjz/",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "exclude_globs": [
                "https://gist.github.com/gists/*/edit*"
            ],
            "css": [
                "css/bannedList.css"
            ],
            "js": [
                "js/jquery-1.7.1.min_plus_highlight.js",
                "js/bannedList.js"
            ]
        }
    ],
    "permissions": []
}