{
    "name": "#BannedList Highlighter",
    "version": "1.0",
    "description": "Highlight #BannedList phrases on every page you read.",
    "browser_action": {
        "default_icon": "icon.png",
        "popup": "popup.html"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/mystyles.css"],
      "js": ["js/jquery-1.7.1.min.js", "js/myscript.js"]
    }
  ],
    "permissions": [
        "http://api.flickr.com/"
    ]
}